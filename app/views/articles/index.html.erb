<h1>Articles Listing Page</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Action</th>
    </tr>
  </thead>

  <tbody>
    <% @article.each do |article| %>
    <tr>
      <td><%= article.title%></td>
      <td><%= article.description%></td>
      <td>Placeholder Action</td>
    </tr>
    <% end %>
  </tbody>
</table>